<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="sort-only" page-range-format="expanded" initialize-with-hyphen="false" default-locale="en-US">
  <info>
    <title>AK's Homework Style</title>
    <title-short>AKHS</title-short>
    <id>http://www.zotero.org/styles/aks-homework-style</id>
    <link href="http://www.zotero.org/styles/aks-homework-style" rel="self"/>
    <author>
      <name>ChatGPT</name>
      <email>support@example.com</email>
    </author>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <updated>2024-05-01T14:31:45+00:00</updated>
    <rights>This work is licensed under a Creative Commons Attribution-ShareAlike 4.0 International License</rights>
  </info>

  <locale xml:lang="en">
    <terms>
      <term name="et-al">et al.</term>
    </terms>
  </locale>
  
  <locale xml:lang="ja">
    <terms>
      <term name="et-al">ä»–</term>
    </terms>
  </locale>

  <macro name="author">
    <names variable="author">
      <name form="long" and="text" delimiter-precedes-last="never" initialize-with=". " name-as-sort-order="all"/>
      <label form="short" prefix=", "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <text variable="title"/>
      </substitute>
    </names>
  </macro>

  <macro name="title">
    <choose>
      <if type="book report thesis" match="any">
        <text variable="title" font-style="italic"/>
      </if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>

  <citation et-al-min="4" et-al-use-first="3" et-al-subsequent-min="4" et-al-subsequent-use-first="3">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout prefix="[" suffix="]" delimiter="; ">
      <text variable="citation-number"/>
      <group>
        <text macro="author" prefix=" " suffix=", "/>
        <text variable="issued-year" prefix=" " suffix=". "/>
        <text macro="title"/>
      </group>
    </layout>
  </citation>

  <bibliography et-al-min="4" et-al-use-first="3">
    <layout>
      <choose>
        <if locale="ja">
          <text macro="author" suffix=", "/>
          <text term="et-al"/>
          <text macro="title" prefix=". " suffix=". "/>
          <date variable="issued" date-parts="year" prefix="(" suffix=")."/>
        </if>
        <else>
          <text macro="author" suffix=", "/>
          <text term="et-al"/>
          <text macro="title" prefix=". " suffix=". "/>
          <date variable="issued" date-parts="year" prefix="(" suffix=")."/>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>
